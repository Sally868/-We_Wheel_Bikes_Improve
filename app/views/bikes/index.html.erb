<p id="notice"><%= notice %></p>
<h1>Bikes</h1>
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Address</th>
      <th>Description</th>
      <th>Price</th>
      <th>Category</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @bikes.each do |bike| %>
      <tr>
        <td><%= bike.name %></td>
        <td><%= bike.address %></td>
        <td><%= bike.description %></td>
        <td><%= bike.price %></td>
        <td><%= bike.category %></td>
        <td><%= link_to 'Show', bike %></td>
        <td><%= link_to 'Edit', edit_bike_path(bike) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
